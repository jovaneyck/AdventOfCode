let rec calculate_rec input up down = 
    match input with
    | [] -> up - down
    | '(' :: rest -> calculate_rec rest (up + 1) down
    | ')' :: rest -> calculate_rec rest up (down + 1)

let calculate_floor input =
    calculate_rec input 0 0

let result = calculate_floor [for c in input -> c]